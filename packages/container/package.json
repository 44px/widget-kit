{
  "name": "@widget-kit/container",
  "version": "1.0.0-rc.0",
  "description": "Isolated environment for embeddable widget via iframe",
  "keywords": [
    "widget",
    "embed",
    "postmessage",
    "iframe",
    "widget-kit"
  ],
  "author": "Alexander Shepelin <Brightcor@gmail.com> (https://github.com/44px)",
  "repository": {
    "type": "git",
    "url": "git://github.com/44px/widget-kit.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src/!(*.test.ts)"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "fmt": "prettier --write 'src/**/*.{ts,json}'",
    "fmt-check": "prettier --list-different 'src/**/*.{ts,json}'",
    "lint": "tslint --project tsconfig.json -t codeFrame src/**/*.ts",
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "tsc -d && rollup -c rollup.config.js",
    "postbuild": "rimraf dist/lib"
  },
  "dependencies": {
    "@widget-kit/rpc": "^1.0.0-rc.0"
  }
}
